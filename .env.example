FIREBASE_ANDROID_PROJECT_ID=your_android_project_id_here
FIREBASE_ANDROID_STORAGE_BUCKET=your_android_storage_bucket_here

# Firebase Configuration - iOS
FIREBASE_IOS_API_KEY=your_ios_firebase_api_key_here
FIREBASE_IOS_APP_ID=your_ios_firebase_app_id_here
FIREBASE_IOS_MESSAGING_SENDER_ID=your_ios_messaging_sender_id_here
