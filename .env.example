# API Keys and Secrets Template
# Copy this file to .env and fill in your actual values
# DO NOT COMMIT .env TO GIT

# RevenueCat API Keys
REVENUECAT_APPLE_API_KEY=your_apple_revenuecat_key_here
REVENUECAT_GOOGLE_API_KEY=your_google_revenuecat_key_here

# Firebase Configuration - Android
FIREBASE_ANDROID_API_KEY=your_android_firebase_api_key_here
FIREBASE_ANDROID_APP_ID=your_android_firebase_app_id_here
